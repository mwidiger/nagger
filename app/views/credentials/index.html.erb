<%= form_with url: messages_path, method: :post, data: { turbo: false } do |form| %>
  <%= form.label :account_sid, "Enter Twilio account SID:" %>
  <%= form.text_field :account_sid %>

  <%= form.label :auth_token, "Enter Twilio Auth Token:" %>
  <%= form.text_field :auth_token %>

  <%= form.hidden_field :source_id, "ragiuhergauirehg" %>

  <%= form.submit "Submit" %>
<% end %>
